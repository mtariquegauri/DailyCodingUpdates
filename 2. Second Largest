

//User function Template for Java

class Solution {
    int print2largest(int arr[], int n) {
        Arrays.sort(arr);
        int max=arr[n-1];
        for(int i=n-2 ; i>=0 ; i--){
            if(arr[i]==max){
                continue;
            }
            else{
                return arr[i];
                
            }
        }
        return -1;
    }
}

